Cell[BoxData[
 RowBox[{
  RowBox[{"pseudoequiv", "[", 
   RowBox[{"base_Integer", ",", "exp_Integer", ",", "coef_Integer"}], "]"}], ":=",
   "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
