Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"pseudopforte", "[", 
    RowBox[{
     RowBox[{"n_Integer", "/;", 
      RowBox[{
       RowBox[{"n", "\[GreaterEqual]", "3"}], "&&", 
       RowBox[{"OddQ", "[", "n", "]"}]}]}], ",", 
     RowBox[{"b_Integer", "/;", 
      RowBox[{"b", "\[GreaterEqual]", "2"}]}]}], "]"}], "/;", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"GCD", "[", 
     RowBox[{"b", ",", "n"}], "]"}], "\[Equal]", "1"}]}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"t", ",", "a", ",", "x", ",", "i"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"t", "=", 
      RowBox[{"n", "-", "1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"a", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"EvenQ", "[", "t", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"t", "=", 
         RowBox[{"t", "/", "2"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"a", "=", 
         RowBox[{"a", "+", "1"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"x", "=", 
      RowBox[{"PowerMod", "[", 
       RowBox[{"b", ",", "t", ",", "n"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "\[Equal]", "1"}], "||", 
        RowBox[{"x", "\[Equal]", 
         RowBox[{"n", "-", "1"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "True", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", 
          RowBox[{"i", "\[LessEqual]", 
           RowBox[{"a", "-", "1"}]}], ",", 
          RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"x", "=", 
            RowBox[{"Mod", "[", 
